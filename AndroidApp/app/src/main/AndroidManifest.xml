<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="fuzihao.test1">

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/images"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/myTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity
            android:name=".Activity.PhotoWallActivity"
            android:theme="@style/myTheme" />
        <activity
            android:name=".Activity.VRActivity"
            android:theme="@style/myTheme" />
        <activity
            android:name=".Activity.MapActivity"
            android:theme="@style/myTheme" />
        <activity
            android:name=".Activity.SelectActivity"
            android:theme="@style/Transparent" />
        <activity
            android:name=".Activity.Setting"
            android:theme="@style/Transparent" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".Music.FloatingMusicPlayerService" />
    </application>

</manifest>